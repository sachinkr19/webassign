<%- include('../partials/header')%>
<div class="container">
    <h1 class="display-5 text-center">Edit</h1>
    <a href="/" class="btn btn-sm mt-2" style="background-color: #7E57C2;color:white">Homepage</a>
    <div class="row">
        <div class="col-md-6 col-sm-12 mt-3">
          <form action="/logs/<%=val._id%>?_method=PATCH" method="POST">
            <div class="mb-3">
                <label for="name" class="form-label">Name</label>
                <input type="text" class="form-control" id="name" name="name" placeholder="name" value="<%=val.name%>"required>
              </div>
              <div class="mb-3">
                <label for="email" class="form-label">Email</label>
                <input type="email" class="form-control" id="email"  name="email" placeholder="email"  value="<%=val.email%>"required>
              </div>
              <div class="mb-3">
                <label for="phone" class="form-label">Mob. No</label>
                <input type="tel" max="10" class="form-control" id="phone"  name="phone" placeholder="Mobile number"  value="<%=val.phone%>"required>
              </div>
              <div class="mb-3">
                <label for="time" class="form-label">Entry Time</label>
                <input id="time"  class="form-control" type="time" name="time"  value="<%=val.time%>">
              </div>
              <div class="mb-3">
                <select class="form-select"  name="body" aria-label="Default select example">
                  <option selected>Body Part to train</option>
                  <option value="Chest">Chest</option>
                  <option value="Back">Back</option>
                  <option value="Shoulder">Shoulder</option>
                  <option value="Biceps">Biceps</option>
                  <option value="Triceps">Triceps</option>
                  <option value="legs">Legs</option>
                </select>
              </div>
              <button type="submit" class="btn btn-md" style="background-color: #7E57C2;color:white">Submit</button>
          </form>
        </div>
        <div class="col-md-6 col-sm-12 mt-3 mb-3 d-flex justify-content-center align-items-center">
            <img src="data:image/png;base64," class="img-fluid" width="80%"alt="">
        </div>
    </div>
</div>
<%- include('../partials/footer')%>
